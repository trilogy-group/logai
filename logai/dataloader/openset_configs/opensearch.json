{
    "log_type": "log",
    "dimensions": {
      "timestamp": ["@timestamp"],
      "body": ["log"],
      "attributes": ["kubernetes.host"]
    },
    "infer_datetime": true,
    "datetime_format": "%Y%m%dT%H:%M:%S:%f",
    "reader_args": {
      "log_format": "<Date> <Time> <Pid> <Level> <Content>"
    }
  }